nav::after,ul::after,li::after{
  content: "";
  clear: both;
  display: table;
}
nav{
  background: rgba(#fff,.5);
  height: auto;
  text-align: center;

  ul{
    list-style: none;
    margin: 0 auto 0 auto;
    width: 450px;
    padding: 0;
    li{
      float: left;
    }
  }

  a{
    display: block;
    padding: 8px 20px;
    text-decoration: none;
  }
}